<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Automation Exercise Regression Suite" parallel="tests" thread-count="3">
    <test name="chrome Regression Test">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="tests.UserRegisterParallelTesting"></class>
        </classes>
    </test>
    <test name="firefox Regression Test">
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="tests.UserRegisterParallelTesting"></class>
        </classes>
    </test>
    <test name="edge Regression Test">
        <parameter name="browser" value="edge"></parameter>
        <classes>
            <class name="tests.UserRegisterParallelTesting"></class>
        </classes>
    </test>
</suite>

